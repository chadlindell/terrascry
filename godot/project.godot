[application]

config/name="GeoSim"
config/description="Physics-realistic geophysical simulation viewer"
config/version="0.1.0"
run/main_scene="res://scenes/main.tscn"
config/features=PackedStringArray("4.3", "Forward+")
config/icon="res://icon.svg"

[autoload]

PhysicsClient="*res://scripts/physics_client.gd"

